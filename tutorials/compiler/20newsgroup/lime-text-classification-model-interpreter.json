              "model_interpret_k_value": 5,
              "model_interpret_top_value": 15,
              "num_of_class": 1,
              "valid_x": "var:X_test",
              "valid_y": "var:y_test",
              "error_analysis_stats_type": "average_score",
              "error_analysis_k_value": 5,
              "error_analysis_top_value": 15
            }
          }
        }
      ]
    }
  ],
  "writers": [
    {
      "class": "Pdf",
      "attr": {
        "name": "20newsgroup-clsssification-model-interpreter-report"
      }
    }
  ]
